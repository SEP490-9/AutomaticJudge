</script>
<div class="container-fluid mt-4">
    <div class="table-responsive">
        <table class="table table-hover table-bordered" id="dataTableStudent" width="100%" cellspacing="0">
            <thead>
                <tr>
                    <th>ID</th>
                    <th>Roll number</th>
                    <th>Name</th>
                </tr>
            </thead>
            <tbody>
                <% for (let i = 0, l = data.length; i < l; ++i) {%>
                <tr>
                    <td><%= data[i].id %></td>
                    <td><%= data[i].rollnumber %></td>
                    <td><%= data[i].name %></td>
                </tr>
                <% } %>
            </tbody>
        </table>
    </div>
</div>
